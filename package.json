{
  "name": "project-name",
  "version": "1.0.0",
  "description": "Project description",
  "main": "stylelint.config.js",
  "dependencies": {},
  "devDependencies": {
    "html5-validator": "^1.2.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0"
  },
  "scripts": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/**/*.css": [
      "npx stylelint --fix"
    ],
    "**/**/*.html": [
      "npx html5v"
    ],
    "**/*.+(js|jsx|json|css|md|html)": [
      "npx prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/user-name/repo-name.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {},
  "homepage": "https://user-name.github.io/repo-name#readme"
}
